{
    "editor.definitionLinkOpensInPeek": true,
    "workbench.editor.enablePreview": false,
    "python.languageServer": "Pylance",
    "jupyter.debugJustMyCode": false,
    "djangointellisense.settingsModule": "${workspaceFolder}/src/healthware/settings.py",
    "python.analysis.extraPaths": [
        "${workspaceFolder}",
        "${workspaceFolder}/src",
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "edraw": true,
        "Include": true,
        "Lib": true,
        "Scripts": true,
        "**/__pycache__": true,
        "**/__init__.py": true,
        "**/migrations": true,
        "**/apps.py": true,
        "**/tests.py": true,
        "manage.py": true,
        "**/wsgi.py": true,
        "**/run.bat": true,
        "**/start.bat": true,
        "**/delete.*": true,
        "**/19_paper.*": true,
        "**/google-services.json": true,
        "**/req.txt": true,
        "**/src/.vscode": true,
        "**/pyvenv.cfg": true
    },
    "code-runner.executorMap": {
        "python": "$pythonPath ${workspaceFolder}/src/manage.py runserver",
    },
    "vsintellicode.features.python.deepLearning": "enabled",
    "typescript.workspaceSymbols.scope": "currentProject",
    "vsintellicode.java.completionsEnabled": false,
    "djangointellisense.projectRoot": "${workspaceFolder}/src",
    "editor.fontSize": 20,
}